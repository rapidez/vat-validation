{
    "name": "rapidez/vat-validation",
    "description": "Implements VIES validation to use on VAT fields in Rapidez",
    "keywords": [
        "rapidez",
        "vat-validation"
    ],
    "homepage": "https://github.com/rapidez/vat-validation",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jade Geels",
            "email": "jade@justbetter.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ibericode/vat": "^2.0",
        "rapidez/core": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\VatValidation\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\VatValidation\\VatValidationServiceProvider"
            ]
        }
    }
}
